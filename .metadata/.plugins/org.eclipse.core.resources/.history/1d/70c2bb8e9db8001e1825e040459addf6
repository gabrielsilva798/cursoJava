package desafio4OrientavcaoObjeto;

public class NumeroUltil {
	int maiorNumeroInteiro(int num1, int num2) {
		return num1 > num2 ? num1 : num2;
	}
	
	int maiorNumeroInteiro(int num1, int num2, int num3) {
		int maiorValor;
		if (num1 > 2 && num1 > 3) {
			maiorValor = num1;
		} else if (num2 > num1 && num2 > num3){
			maiorValor = num2;
		} else {
			maiorValor = num3;
		}
		
		return maiorValor;
		
	}
}
